<odoo>
	<data>
		<record id="fish_campaign_sequence" model="ir.sequence">
            <field name="name">Fish Campaign Sequence</field>
            <field name="code">fishing.campaign</field>
            <field name="prefix">FC/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>
        <record id="fish_campaign_run_sequence" model="ir.sequence">
            <field name="name">Fish Campaign Run Sequence</field>
            <field name="code">fishing.campaign.run</field>
            <field name="prefix">FCR/%(year)s%(month)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>
	</data>
</odoo>